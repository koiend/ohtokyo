{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "name": "inputDocumentIn",
      "databaseName": "outDatabase",
      "collectionName": "MyCollection",
      "userId": "{Query.userId}",
      "createIfNotExists": true,
      "connectionStringSetting": "task2_DOCUMENTDB",
      "direction": "in"
    },
    {
      "type": "cosmosDB",
      "name": "inputDocumentOut",
      "databaseName": "outDatabase",
      "collectionName": "MyCollection",
      "createIfNotExists": true,
      "connectionStringSetting": "task2_DOCUMENTDB",
      "direction": "out"
    }
  ]
}